{
  "name": "doom-index",
  "version": "2.0.0",
  "description": "A decentralized archive of financial emotions. AI generates one painting every minute, translating the collective psychology of 8 pump.fun tokens into visual art.",
  "private": false,
  "engines": {
    "node": ">=24.11.1"
  },
  "scripts": {
    "postinstall": "bun run patch:opennext",
    "dev": "next dev",
    "build": "next build",
    "build:cf": "opennextjs-cloudflare build",
    "patch:opennext": "bun scripts/patch-opennext-webpack-runtime.ts",
    "start": "next start",
    "lint": "eslint",
    "format": "prettier --check . --experimental-cli",
    "format:fix": "prettier --write . --experimental-cli",
    "test": "bun test --env-file=.dev.vars --preload=./tests/preload.ts",
    "test:unit": "bun test --env-file=.dev.vars --grep 'unit' --preload=./tests/preload.ts",
    "test:integration": "bun test --env-file=.dev.vars --grep 'integration' --preload=./tests/preload.ts",
    "typecheck": "tsgo --noEmit",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts",
    "generate": "bun --env-file=.dev.vars scripts/generate.ts",
    "watch-cron": "bun scripts/watch-cron.ts",
    "truncate-r2": "bun --env-file=.dev.vars scripts/truncate-r2.ts",
    "truncate-r2:dry-run": "bun --env-file=.dev.vars scripts/truncate-r2.ts --dry-run",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "bun --env-file=.dev.vars drizzle-kit migrate",
    "db:migrate:prod": "NODE_ENV=production bun run db:migrate",
    "db:push": "bun --env-file=.dev.vars drizzle-kit push",
    "db:push:prod": "NODE_ENV=production bun run db:push",
    "db:studio": "drizzle-kit studio",
    "db:studio:prod": "NODE_ENV=production bun run db:studio",
    "preview": "opennextjs-cloudflare preview --env=dev --test-scheduled",
    "upload": "opennextjs-cloudflare upload",
    "deploy": "opennextjs-cloudflare deploy"
  },
  "dependencies": {
    "@coingecko/coingecko-typescript": "^2.3.0",
    "@mdx-js/loader": "^3.1.1",
    "@mdx-js/react": "^3.1.1",
    "@metaplex-foundation/mpl-token-metadata": "^3.4.0",
    "@metaplex-foundation/umi": "^1.4.1",
    "@metaplex-foundation/umi-bundle-defaults": "^1.4.1",
    "@metaplex-foundation/umi-signer-wallet-adapters": "^1.4.1",
    "@metaplex-foundation/umi-uploader-irys": "^1.4.1",
    "@metaplex-foundation/umi-web3js-adapters": "^1.4.1",
    "@next/mdx": "^16.0.6",
    "@next/third-parties": "^16.0.6",
    "@opennextjs/cloudflare": "^1.14.1",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.4.2",
    "@solana/wallet-adapter-base": "^0.9.27",
    "@solana/wallet-adapter-react": "^0.15.39",
    "@solana/wallet-adapter-react-ui": "^0.9.39",
    "@solana/wallet-adapter-wallets": "^0.19.37",
    "@solana/web3.js": "^1.98.4",
    "@t3-oss/env-nextjs": "^0.13.8",
    "@tanstack/react-query": "^5.90.11",
    "@tavily/core": "^0.5.13",
    "@trpc/client": "^11.7.2",
    "@trpc/next": "^11.7.2",
    "@trpc/react-query": "^11.7.2",
    "@trpc/server": "^11.7.2",
    "@trpc/tanstack-react-query": "^11.7.2",
    "drizzle-orm": "^0.44.7",
    "neverthrow": "^7.2.0",
    "next": "^16.0.6",
    "pinata": "^2.5.1",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "sonner": "^2.0.7",
    "three": "^0.181.2",
    "three-stdlib": "^2.36.1",
    "use-haptic": "^1.1.13",
    "use-sound": "^5.0.0",
    "valibot": "^1.2.0",
    "wrangler": "^4.51.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20251202.0",
    "@happy-dom/global-registrator": "^20.0.11",
    "@prettier/plugin-oxc": "^0.1.2",
    "@tailwindcss/postcss": "^4.1.17",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@types/bun": "^1.3.3",
    "@types/node": "^20.19.25",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/three": "^0.181.0",
    "@typescript/native-preview": "^7.0.0-dev.20251202.1",
    "babel-plugin-react-compiler": "^1.0.0",
    "baseline-browser-mapping": "^2.8.32",
    "better-sqlite3": "^12.5.0",
    "drizzle-kit": "^0.31.7",
    "eslint": "^9.39.1",
    "eslint-config-next": "16.0.1",
    "eslint-plugin-react-compiler": "^19.1.0-rc.2",
    "eslint-plugin-tailwindcss": "^4.0.0-beta.0",
    "eslint-plugin-unused-imports": "^4.3.0",
    "next-rspack": "^16.0.6",
    "prettier": "^3.7.3",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3"
  },
  "packageManager": "bun@1.3.3",
  "prettier": {
    "semi": true,
    "singleQuote": false,
    "printWidth": 120,
    "trailingComma": "all",
    "arrowParens": "avoid",
    "bracketSpacing": true,
    "tabWidth": 2,
    "useTabs": false,
    "plugins": [
      "prettier-plugin-organize-imports",
      "prettier-plugin-tailwindcss",
      "@prettier/plugin-oxc"
    ]
  }
}
