NODE_ENV="development"
NEXTJS_ENV="development"

NEXT_PUBLIC_BASE_URL="http://localhost:8787"

LOG_LEVEL=DEBUG

IMAGE_MODEL="runware:106@1"

NEXT_PRIVATE_DEBUG_CACHE=1

# R2 Public URL (optional)
# For production: Use your custom domain (e.g., "https://storage.doomindex.fun")
# For development: Use local endpoint (e.g., "http://localhost:8787/api/r2") or leave empty
# Supports both formats:
#   - With protocol: "https://storage.doomindex.fun" or "http://localhost:8787/api/r2"
#   - Without protocol: "storage.doomindex.fun" (will default to https)
NEXT_PUBLIC_R2_URL=""

# API Keys (only required when using specific providers)
# OpenAI API Key (required when IMAGE_PROVIDER=openai)
OPENAI_API_KEY="dummy_openai_api_key"

# Runware API Key (required when IMAGE_PROVIDER=runware)
RUNWARE_API_KEY="dummy_runware_api_key"

# Cloudflare
CLOUDFLARE_ACCOUNT_ID="dummy_cloudflare_account_id"
R2_ACCESS_KEY_ID="dummy_r2_access_key_id"
R2_SECRET_ACCESS_KEY="dummy_r2_secret_access_key"
CLOUDFLARE_D1_TOKEN="dummy_cloudflare_d1_token"
CLOUDFLARE_DATABASE_ID="dummy_cloudflare_database_id"

# Tavily API Key (required for dynamic-prompt feature)
# Get your API key from https://tavily.com/
TAVILY_API_KEY="dummy_tavily_api_key"

# CoinGecko API Key (may be optional)
COINGECKO_API_KEY="dummy_coingecko_api_key"

# Cloudflare Workers AI Token (for running gen-img.ts in local)
CLOUDFLARE_API_TOKEN="dummy_cloudflare_workers_ai_token"

SLACK_WEBHOOK_URL="https://hooks.slack.com/services/hoge/fuga/piyo"

# OpenNext Cache Purge API Keys
CACHE_PURGE_API_TOKEN="dummy_cache_purge_api_token"
CACHE_PURGE_ZONE_ID="dummy_cache_purge_zone_id"

# Admin Secret
ADMIN_SECRET="dummy_admin_secret"

# Pinata API Keys (for minting NFTs)
PINATA_API_KEY="dummy_pinata_api_key"
PINATA_API_SECRET="dummy_pinata_api_secret"
PINATA_JWT="dummy_pinata_jwt"