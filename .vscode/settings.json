{
  "files.associations": {
    "*.*.vars": "dotenv",
    "*.mdc": "markdown"
  },

  "editor.codeActionsOnSave": {
    "quickfix.quickfix": "explicit",
    "source.organizeImports.quickfix": "explicit"
  },

  "typescript.preferences.includePackageJsonAutoImports": "auto",
  "typescript.suggest.autoImports": true,
  "cSpell.words": ["Kontext", "runware"]
}
